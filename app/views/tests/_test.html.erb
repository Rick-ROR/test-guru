<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= test.author.name %></td>
  <td><%= link_to 'Прочитать', test_path(test) %></td>
  <td><%= link_to 'Изменить', edit_test_path(test) %></td>
  <td>
    <%= link_to 'Удалить',
                test_path(test),
                method: :delete,
                data:	{	confirm:	'Вы уверены?' }	%>
    </td>
  <td><%= button_to 'Пройти', start_test_path(test) %></td>
</tr>
