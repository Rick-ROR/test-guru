<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= test.author.name %></td>
  <td><%= link_to t('show'), admin_test_path(test), class: "btn btn-info" %></td>
  <td><%= link_to t('edit'), edit_admin_test_path(test), class: "btn btn-secondary" %></td>
  <td>
    <%= link_to t('delete'),
                admin_test_path(test),
                method: :delete,
                data:	{	confirm: t('.confirm') }, class: "btn btn-warning"	%>
    </td>
</tr>
